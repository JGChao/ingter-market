/* uinfor */
.uinfor{
	width: 690rpx;
	margin: 30rpx auto;
	border-radius: 10rpx;
	-webkit-box-shadow: 0 0 7rpx 2rpx #F7F7F7;
	        box-shadow: 0 0 7rpx 2rpx #F7F7F7;
	background: #fff;
	padding: 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
