.bigBox{
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	background: #F7F7F7;
}

/* consignee */
.consignee{
	width: 690rpx;
	border-radius: 10rpx;
	margin: 40rpx auto;
	padding: 40rpx 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	background: #fff;
	-webkit-box-shadow: 0 0 10rpx 2rpx #E7E7E7;
	        box-shadow: 0 0 10rpx 2rpx #E7E7E7;
}

/* consig */
.consignee .consig{
	width: 650rpx;
	padding:0 120rpx 0 62rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	margin-bottom: 20rpx;
}
.consignee .consig .left{
	font-size: 30rpx;
	font-weight: bold;
	color: #000;
}
.consignee .consig .right{
	font-size: 26rpx;
	color: #333;
}

/* conaddress */
.consignee .conaddress{
	width: 650rpx;
	height: 43rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	margin-bottom: 38rpx;
}
.consignee .conaddress .left{
	width: 62rpx;
	height: 100%;
}
.consignee .conaddress .left image{
	width: 43rpx;
	height: 43rpx;
}
.consignee .conaddress .right{
	width: 588rpx;
	padding-top: 13rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	height: 100%;
	font-size: 26rpx;
	color: #333;
	overflow: hidden;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	white-space: nowrap;
}

/* shopinfor */
.consignee .shopinfor{
	width: 650rpx;
}

/* upward */
.consignee .shopinfor .upward{
	width: 100%;
	height: 200rpx;
	padding-left: 9rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	margin-bottom: 50rpx;
}
.consignee .shopinfor .upward .sthumb{
	width: 280rpx;
	height: 200rpx;
	border-radius: 7rpx;
	float: left;
}
.consignee .shopinfor .upward .sthumb image{
	width: 100%;
	height: 100%;
}
.consignee .shopinfor .upward .infor{
	width: 361rpx;
	height: 200rpx;
	padding: 46rpx 0 0 36rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	float: left;
}
.consignee .shopinfor .upward .infor text{
	display: block;
}
.consignee .shopinfor .upward .infor .chname{
	width: 325rpx;
	font-size: 28rpx;
	color: #333;
	margin-bottom: 40rpx;
	overflow: hidden;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	white-space: nowrap;
}
.consignee .shopinfor .upward .infor .innum{
	width: 325rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.consignee .shopinfor .upward .infor .innum .left{
	font-size: 26rpx;
	color: #333;
}
.consignee .shopinfor .upward .infor .innum .right{
	font-size: 30rpx;
	color: #666;
	margin-right: 15rpx;
}

/* down */
.consignee .shopinfor .down{
	width: 100%;
	text-align: right;
}
.consignee .shopinfor .down .total{
	font-size: 28rpx;
	color: #333;
}
.consignee .shopinfor .down .integ{
	font-size: 28rpx;
	color: #FB4B5C;
	margin-right: 15rpx;
}

/* orderinfor */
.orderinfor{
	width: 690rpx;
	padding: 40rpx 40rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	border-radius: 10rpx;
	background: #fff;
	-webkit-box-shadow: 0 0 10rpx 2rpx #E7E7E7;
	        box-shadow: 0 0 10rpx 2rpx #E7E7E7;
	margin: auto;
}
.orderinfor .orin{
	display: block;
	font-size: 26rpx;
	color: #333;
	margin-bottom: 30rpx;
}
.orderinfor .orin:last-child{
	margin-bottom: 0;
}

/* tabbar */
.tabbar{
	width: 750rpx;
	height: 98rpx;
	background: #FB4B5C;
	text-align: center;
	line-height:98rpx ;
	font-size: 30rpx;
	color: #fff;
	position: fixed;
	bottom: 0;
	left: 0;
	-webkit-box-shadow: 0 -2rpx 10rpx 2rpx #D4D4D3;
	        box-shadow: 0 -2rpx 10rpx 2rpx #D4D4D3;
}

