.bigBox{
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	background: #fff;
}
.scroll{
	width: 100%;
	height: auto;
	background: #fff;
	overflow: auto;
	padding-bottom: 196rpx;
}
/* conList */
.conList{
	width: 690rpx;
	height: 240rpx;
	background: #fff;
	border-radius: 10rpx;
	padding-top:38rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	margin:30rpx auto 0 auto;
	-webkit-box-shadow: 0 0 7rpx 2rpx #EFEEEE;
	        box-shadow: 0 0 7rpx 2rpx #EFEEEE;
	overflow: hidden;
	overflow: auto;
}
.conList::-webkit-scrollbar{
	display: none;
}
.conList .scoll{
	width: 750rpx;
	height: 164rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.conList .scoll .conleft{
	width: 164rpx;
	height: 164rpx;
	margin: 0 30rpx 0 0;
}
.conList .scoll .conleft image{
	width: 100%;
	height: 100%;
}
.conList .scoll .conright{
	width: 427rpx;
	height: 164rpx;
}
.conList .scoll .conright .upper{
	width: 100%;
	height: 80rpx;
	margin-bottom: 44rpx;
}
.conList .scoll .conright .upper .title{
	display: block;
	width: 100%;
	font-size: 30rpx;
	color: #333;
	overflow: hidden;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	white-space: nowrap;
}
.conList .scoll .conright .upper .integr{
	width: 100%;
	font-size: 30rpx;
	color: #FB4B5C;
	font-weight: bold;
}
.conList .scoll .conright .upper .integr .orgprice{
	font-size: 24rpx;
	color: #333;
	font-weight: 100;
	margin-left: 20rpx;
	text-decoration: line-through;
}
.conList .scoll .conright .lower{
	width: 100%;
	height: 40rpx;
}
.conList .scoll .conright .lower .addsub{
	width: 162rpx;
	height: 40rpx;
	float: right;
	margin-right: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.conList .scoll .conright .lower .addsub .sub{
	width: 20rpx;
	height: 31rpx;
}
.conList .scoll .conright .lower .addsub .sub image{
	display: block;
	margin-top: -0.5rpx;
	width: 100%;
	height: 100%;
}
.conList .scoll .conright .lower .addsub .num{
	width: 100rpx;
	height: 40rpx;
	border: 1rpx solid #666666;
	text-align: center;
	line-height: 40rpx;
	color: #FB4B5C;
	font-weight: bold;
}
.conList .scoll .remove{
	width: 60rpx;
	height: 30rpx;
}
.conList .scoll .remove image{
	display: block;
	width: 27rpx;
	height: 30rpx;
	margin: auto;
}
.scroll .conList:last-child{
	margin-bottom: 5rpx;
}
/* settle */
.settle{
	width: 750rpx;
	height: 98rpx;
	background: #f7f7f7;
	padding: 0 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	position:fixed;
	bottom:0px;
	left: 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.settle .sbtnbox{
	width: auto;
	height: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.settle .sbtnbox .total{
	font-size: 26rpx;
	color: #333;
}
.settle .sbtnbox .sbtn{
	width: 120rpx;
	height: 60rpx;
	border-radius: 30rpx;
	background: #5E5FF7;
	color: #fff;
	font-size: 28rpx;
	text-align: center;
	line-height: 60rpx;
	margin-left: 10rpx;
}
.settle /deep/ .uni-checkbox-group{
	font-size: 22rpx;
}

